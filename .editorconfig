# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true

[*.{js,ts}]
indent_size = 2
indent_style = space


[*.{json,yml}]
indent_size = 4
indent_style = space

[.prettierrc]
indent_size = 4
indent_style = space
